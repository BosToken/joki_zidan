{
    "version" : 2,
    "functions" : {
        "api/index.php":{
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes" : [
        {
            "src": "/img/(.*)",
            "dest": "/public/img/"
        },
        {
            "src": "/css/(.*)",
            "dest": "/public/css/"
        },
        {
            "src": "/fonts/(.*)",
            "dest": "/public/fonts/"
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/"
        },
        {
            "src": "/js/(.*)",
            "dest": "/public/js/"
        },
        {
            "src": "/vendor/(.*)",
            "dest": "/public/vendor/"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://peminjamanbuku.vercel.app",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    },
    "outputDirectory": "public"
}